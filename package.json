{
  "name": "boilerplate-fullstack-routing",
  "homepage": "https://wehikenewzealand.com/",
  "type": "module",
  "version": "2.0.0",
  "main": "server/index.js",
  "license": "MIT",
  "keywords": [
    "fullstack",
    "routing"
  ],
  "scripts": {
    "test": "vitest",
    "start": "node dist/server.js",
    "dev": "run-p dev:client dev:server",
    "dev:client": "vite",
    "dev:server": "tsx watch server/index.ts",
    "build": "run-s build:client build:server",
    "build:client": "vite build",
    "build:server": "esbuild --packages=external --platform=node --format=esm --outfile=dist/server.js --bundle server/index.ts",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "knex": "knex --knexfile ./server/db/knexfile.js"
  },
  "eslintConfig": {
    "extends": "@devacademy/eslint-config/react",
    "ignorePatterns": [
      "bundle.js"
    ]
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.2.4",
    "cors": "^2.8.5",
    "dompurify": "^3.2.3",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-jwt": "^7.7.5",
    "express-openid-connect": "^0.5.0",
    "html-react-parser": "^5.2.0",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^2.1.5",
    "knex": "^2.5.1",
    "leaflet": "^1.9.4",
    "pg": "^8.13.1",
    "proj4": "^2.14.0",
    "react-icons": "^5.3.0",
    "react-leaflet": "^4.2.1",
    "sqlite3": "^5.1.6",
    "superagent": "^8.1.2"
  },
  "devDependencies": {
    "@devacademy/eslint-config": "^1.10.1",
    "@tanstack/eslint-plugin-query": "^5.12.2",
    "@tanstack/react-query": "^5.12.2",
    "@tanstack/react-query-devtools": "^5.13.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.14",
    "@types/leaflet": "^1.9.14",
    "@types/proj4": "^2.5.5",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.0.11",
    "@types/superagent": "^8.1.1",
    "@types/supertest": "^6.0.2",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.20",
    "daisyui": "^4.12.23",
    "esbuild": "^0.18.19",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "jsdom": "^22.1.0",
    "nock": "^13.5.6",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.49",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.13",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.11.2",
    "sass": "^1.50.1",
    "supertest": "^6.3.4",
    "tailwindcss": "^3.4.15",
    "tsx": "^3.12.7",
    "typescript": "^5.3.3",
    "vite": "^5.0.0",
    "vitest": ">=1.6.1"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "plugins": [
      "prettier-plugin-tailwindcss"
    ]
  },
  "browserslist": "> 2%, not dead"
}
